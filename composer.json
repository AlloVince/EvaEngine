{
  "name": "evaengine/evaengine",
  "description": "A development engine based on Phalcon Framework",
  "license": "BSD-3-Clause",
  "authors": [
    {
      "name": "AlloVince",
      "email": "allo.vince@gmail.com",
      "homepage": "http://avnpc.com"
    }
  ],
  "require": {
    "php": ">=5.5",
    "ext-phalcon": ">=2.0.8",
    "psr/http-message": "~1.0",
    "swiftmailer/swiftmailer": "~5.3",
    "knplabs/gaufrette": "~0.1",
    "symfony/console": "2.7.*",
    "symfony/filesystem": "2.7.*",
    "symfony/finder": "2.7.*",
    "symfony/process": "2.7.*",
    "nikic/php-parser": "1.4.*",
    "evaengine/eva-sms": "0.2.*"
  },
  "require-dev": {
    "phpunit/phpunit": "5.0.*",
    "mockery/mockery": "0.9.*",
    "satooshi/php-coveralls": "0.6.*"
  },
  "bin": [
    "engine"
  ],
  "suggest": {
    "ext-redis": "*",
    "ext-gearman": "*"
  },
  "autoload": {
    "psr-4": {"Eva\\EvaEngine\\": "src/EvaEngine"},
    "files": ["src/EvaEngine/helpers.php"]
  }
}
